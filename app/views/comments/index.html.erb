<div class="all-comments">
  <h2>"Would you rather <%= @question.option1 %> or <%= @question.option2 %></h2>
  <h3>Comments:</h3>
    <% @comments.each do |comment| %>
      <%= render partial: 'comment', locals: {comment: comment} %>
    <% end %>
</div>
<br>
<div class="paginate">
  <%= paginate @comments %>
</div>
